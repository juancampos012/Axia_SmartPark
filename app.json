{
  "expo": {
    "name": "AxiaSmartPark",
    "slug": "AxiaSmartPark",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/Axia_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "AxiaSmartPark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "icon": "./assets/Axia_icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.AxiaSmartPark",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicación para mostrarte los parqueaderos más cercanos y calcular distancias precisas.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Axia SmartPark necesita acceso a tu ubicación para mostrarte parqueaderos cercanos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Axia_icon.png",
        "backgroundColor": "#10B981"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.AxiaSmartPark",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite a $(PRODUCT_NAME) acceder a tu ubicación para mostrarte parqueaderos cercanos.",
          "locationWhenInUsePermission": "Permite a $(PRODUCT_NAME) acceder a tu ubicación mientras usas la app.",
          "isAndroidBackgroundLocationEnabled": false,
          "isAndroidForegroundServiceEnabled": false
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9361cb13-b2e6-42c9-b3b9-f47f392079bf"
      }
    }
  }
}
